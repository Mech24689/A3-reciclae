{
  "name": "api-reciclagem-cidada",
  "version": "1.0.0",
  "description": "API para o sistema de gestão de reciclagem cidadã.",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --files --respawn src/server.ts",
    "build": "tsc",
    "knex:migrate": "knex --knexfile db/knexfile.ts migrate:latest",
    "knex:rollback": "knex --knexfile db/knexfile.ts migrate:rollback",
    "knex:make": "knex --knexfile db/knexfile.ts migrate:make -x ts",
    "knex:seed": "knex --knexfile db/knexfile.ts seed:run",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "keywords": [
    "typescript",
    "express",
    "knex",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "react-router-dom": "^7.9.5",
    "swagger-ui-react": "^5.30.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^30.0.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
